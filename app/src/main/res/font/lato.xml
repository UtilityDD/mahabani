<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--
    NOTE: You will need to download the Lato font files from Google Fonts (fonts.google.com)
    and place lato_regular.ttf and lato_bold.ttf into this 'font' directory.
    -->

    <!-- regular -->
    <font
        android:font="@font/lato_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/lato_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/lato_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/lato_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>